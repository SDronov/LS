  inherited dsData: TfdcQuery
    SQL.Strings = (
      'select id,'
      '       Name,'
      '       ShortName,'
      '       object_type_id,'
      '       state_id,'
      '       AccessLevel,'
      '       SysName,'
      '       Descript,'
      '       owner_object_id,'
      '       TypeName,'
      '       TypeSysName,'
      '       ObjectKindList,'
      '       custom_mode_id,'
      '       currency_id,'
      '       subject_id,'
      '       customs_code,'
      '       reg_date,'
      '       decl_no,'
      '       modify_date,'
      '       Is_Active,'
      '       activation_date,'
      '       decl_name,'
      '       decl_inn,'
      '       decl_kpp,'
      '       sum_rub,'
      '       curr_code,'
      '       -sum(reserve_summa) AS reserve_summa'
      ''
      '  from (select t.id,'
      '               t.Name,'
      '               t.ShortName,'
      '               t.object_type_id,'
      '               t.state_id,'
      '               t.AccessLevel,'
      '               t.SysName,'
      '               t.Descript,'
      '               t.owner_object_id,'
      '               t.TypeName,'
      '               t.TypeSysName,'
      '               t.ObjectKindList,'
      '               t.custom_mode_id,'
      '               t.currency_id,'
      '               t.subject_id,'
      '               t.customs_code,'
      '               t.reg_date,'
      '               t.decl_no,'
      '               t.modify_date,'
      '               t.Is_Active,'
      '               t.activation_date,'
      '               t.decl_name,'
      '               t.decl_inn,'
      '               t.decl_kpp,'
      '               t.sum_rub,'
      '               c.code            AS curr_code,'
      '               pd.summa          AS reserve_summa'
      '          FROM fdc_rel_act_ded_deduction_lst rd,'
      '               fdc_payment_deduction_all_lst pd,'
      '               fdc_tr_lst                    t,'
      '               fdc_nsi_currency_lst          c'
      '         WHERE rd.object_id = :id'
      '           AND pd.id = rd.rel_object_id'
      '           AND t.id = pd.decl_id'
      '           AND t.currency_id = c.id(+))'
      ' group by id,'
      '          Name,'
      '          ShortName,'
      '          object_type_id,'
      '          state_id,'
      '          AccessLevel,'
      '          SysName,'
      '          Descript,'
      '          owner_object_id,'
      '          TypeName,'
      '          TypeSysName,'
      '          ObjectKindList,'
      '          custom_mode_id,'
      '          currency_id,'
      '          subject_id,'
      '          customs_code,'
      '          reg_date,'
      '          decl_no,'
      '          modify_date,'
      '          Is_Active,'
      '          activation_date,'
      '          decl_name,'
      '          decl_inn,'
      '          decl_kpp,'
      '          sum_rub,'
      '          curr_code')
    ParamData = <
      item
        DataType = ftFloat
        Name = 'id'
        ParamType = ptInput
      end>
    object dsDataCUSTOM_MODE_ID: TFloatField
