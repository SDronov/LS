  inherited dsData: TfdcQuery
    Session = MainData.Session
    SQL.Strings = (
      'SELECT a.id'
      '      ,a.code'
      '      ,a.name'
      '      ,a.parent_id'
      '      ,ir.id AS doc_id'
      '      ,ir.doc_number'
      '      ,ir.doc_date'
      '      ,CASE'
      
        '         WHEN EXISTS( SELECT 0 FROM fdc_acc aa WHERE aa.parent_i' +
        'd = a.id ) THEN'
      
        '           (SELECT SUM(i.rest_sum) FROM fdc_initial_rest i WHERE' +
        ' i.currency_id = :currency_id AND i.account_id IN (SELECT aa.id ' +
        'FROM fdc_acc aa START WITH aa.parent_id = a.id CONNECT BY PRIOR ' +
        'aa.id = aa.parent_id))'
      '         ELSE ir.rest_sum'
      '       END AS rest_sum'
      '  FROM fdc_acc_lst a'
      
        '  LEFT OUTER JOIN fdc_initial_rest_lst ir ON ir.account_id  = a.' +
        'id'
      
        '                                         AND ir.currency_id = :c' +
        'urrency_id')
    SourceServerObject = 'FDC_INITIAL_REST_LST'
    BeforePost = dsDataBeforePost
    AfterPost = dsDataAfterPost
    AfterCancel = dsDataAfterCancel
    ParamData = <
      item
        DataType = ftInteger
        Name = 'currency_id'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'currency_id'
        ParamType = ptInput
      end>
    object dsDataID: TFloatField
      FieldName = 'ID'
    end
    object dsDataCODE: TStringField
      DisplayLabel = #1050#1086#1076
      FieldName = 'CODE'
      Size = 1500
    end
    object dsDataNAME: TStringField
      DisplayLabel = #1053#1072#1079#1074#1072#1085#1080#1077
      FieldName = 'NAME'
      Size = 1500
    end
    object dsDataPARENT_ID: TFloatField
      FieldName = 'PARENT_ID'
    end
    object dsDataDOC_ID: TFloatField
      FieldName = 'DOC_ID'
    end
    object dsDataDOC_NUMBER: TStringField
      DisplayLabel = #8470
      FieldName = 'DOC_NUMBER'
      Size = 100
    end
    object dsDataDOC_DATE: TDateTimeField
      DisplayLabel = #1044#1072#1090#1072
      FieldName = 'DOC_DATE'
    end
    object dsDataREST_SUM: TFloatField
      DisplayLabel = #1053#1072#1095#1072#1083#1100#1085#1099#1081' '#1086#1089#1090#1072#1090#1086#1082
      FieldName = 'REST_SUM'
    end
