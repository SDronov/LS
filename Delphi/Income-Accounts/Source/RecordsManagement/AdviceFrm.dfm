    object sqlGetExistsNoConfirmDoc: TsqlOp
      SQL.Strings = (
        'DECLARE'
        '  Err1  Exception;'
        '  i     Number;'
        'BEGIN'
        'Begin '
        '  FOR r IN (SELECT t.*'
        
          '                  ,(select o.typesysname  from fdc_object_lst o ' +
          'where o.id = t.payment_id) as po_sysname'
        '              FROM fdc_advice_reserve_lst t'
        '             WHERE t.advice_id = :pID'
        '               AND t.edate IS NULL)'
        '  LOOP             '
        '    -- '#1055#1088#1086#1074#1077#1088#1080#1084' '#1077#1089#1083#1080' '#1101#1090#1086' '#1095#1077#1082' '#1090#1086' '#1058#1050' '#1087#1088#1086#1074#1077#1088#1080#1084' '#1087#1086#1086#1090#1074#1077#1088#1078#1076#1077#1085' '#1083#1080' '#1086#1085
        '    If r.po_sysname in ('#39'CustomCheque'#39') Then'
        '       begin'
        '         select id into i'
        '         from FDC_PAYMENT_ORDER_CNFRM_LST c'
        '         where c.confirmed_pay_id = r.payment_id;'
        '       exception'
        '       when no_data_found then'
        '            Raise Err1;'
        '       end;'
        '    end if;'
        '  END LOOP;'
        'Exception'
        '  When Err1 Then'
        '    Fdc_Error_Handler_Pck.Handle_And_Raise('#39'PAY'#39','#39'00137'#39');'
        
          '  When Others Then Fdc_Error_Handler_Pck.Handle_And_Raise('#39'RET'#39',' +
          #39'00073'#39',Sqlerrm,:pID);'
        'End;  '
        'END; ')
    end
