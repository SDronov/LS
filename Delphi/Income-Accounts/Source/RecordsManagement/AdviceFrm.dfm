  inherited dsReport: TfdcQuery
    SQL.Strings = (
      'select d.doc_date,'
      '       d.doc_number,'
      '       d.custom_name,'
      
        '       decode(d.decl_name, null, null, d.decl_name || '#39' '#39') as de' +
        'cl_name,'
      
        '       decode(d.decl_inn, null, null, '#39#1048#1053#1053' '#39' || d.decl_inn || '#39',' +
        ' '#39') as decl_inn,'
      
        '       decode(d.decl_kpp, null, null, '#39#1050#1055#1055' '#39' || d.decl_kpp || '#39',' +
        ' '#39') as decl_kpp,'
      
        '       decode(d.decl_address, null, null, d.decl_address) as dec' +
        'l_address,'
      '       decode(p.doc_no, null, null,'
      
        '                        pt.shortname || '#39' '#39' || p.ser_no || '#39' '#39' |' +
        '| p.doc_no || decode(p.issued_date, null, null,'
      
        '                                                                ' +
        '                     '#39', '#1074#1099#1076'. '#39' || p.issued_date || '#39' '#39' || p.issu' +
        'ed_by) || '#39', '#39') as passport_data,'
      '       ad.name as cash_assets_name,'
      
        '       -( SELECT SUM( ad.summa ) FROM fdc_advice_deduction_lst a' +
        'd WHERE ad.advice_id = d.id) as doc_sum,'
      '       (select t.StringValue from fdc_value_lst t'
      
        '        where t.IsSystem = 1 and t.SYSNAME = '#39'PayBossName'#39') as P' +
        'AY_BOSS_NAME,'
      
        '       ( SELECT MIN( o.TYPENAME ) FROM fdc_advice_deduction_lst ' +
        'ad, fdc_object_lst o WHERE ad.advice_id = d.id AND ad.decl_id = ' +
        'o.id ) AS doc_type'
      '  from fdc_advice_lst d'
      '      ,fdc_person_lst p'
      '      ,fdc_passport_type_lst pt'
      '      ,(select a.name'
      '              ,v.owner_object_id'
      '          from fdc_value_lst v'
      '              ,fdc_assets_dict_lst a'
      '         where v.sysname = '#39'AssetsDictCode'#39
      '           and v.stringvalue = a.code) ad'
      ' where '
      '   d.subject_id = p.id(+)'
      '   and p.doc_type = pt.code(+)'
      '   and d.object_type_id = ad.owner_object_id(+)'
      '   and d.id = :id')
    Left = 136
    Top = 288
    ParamData = <
      item
        DataType = ftFloat
        Name = 'id'
        ParamType = ptUnknown
      end>
  end
