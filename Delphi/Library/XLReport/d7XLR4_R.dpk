{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'XL Report run-time package for Delphi 7.0'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  vcl,
  rtl,
  vclx,
  dbrtl;

contains
  xlStdOPack,
  Office8G2,
  VBIDE8G2,
  xlcClasses,
  xlcOPack,
  xlcUtils,
  xlPivotOPack,
  xlReportG2,
  Excel8G2,
  xlReport,
  xlEngine;

end.
